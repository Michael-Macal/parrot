{
  "name": "axp-mock-middleware",
  "version": "0.0.1",
  "description": "Common selector library for axp-app components using reselect.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib --copy-files",
    "lint": "eslint --ignore-path .gitignore --ext .js src test",
    "test": "BABEL_ENV=test mocha --compilers js:babel-register",
    "test:browser": "karma start",
    "test:node": "MOCHA_FILE=./test-results/node.xml mocha --compilers js:babel-register --reporter mocha-junit-reporter",
    "watch": "nodemon --ignore lib/ --exec npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.aexp.com/~jcros8/axp-mock-middleware.git"
  },
  "contributors": [
    "Jack Cross <jack.cross1@aexp.com>",
    "Nathan Arick Force <Nathan.A.Force@aexp.com>",
    "Jason Schapiro <jason.schapiro@aexp.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "es6-promise": "^3.1.2",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^14.0.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "isparta-loader": "^2.0.0",
    "react": "^15.0.1",
    "webpack": "^1.13.0"
  }
}
