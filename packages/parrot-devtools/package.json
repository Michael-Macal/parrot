{
  "name": "parrot-devtools",
  "version": "2.1.0",
  "private": true,
  "contributors": [
    "Jack Cross <jack.cross1@aexp.com>",
    "Jason Schapiro <jason.schapiro@aexp.com>",
    "Zach Ripka <Zachary.J.Ripka@aexp.com>"
  ],
  "description": "Devtool components for easy scenario switching when developing with Parrot.",
  "scripts": {
    "start": "chokidar 'src/**' -c 'npm run build' --initial",
    "clean": "rimraf build && rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run bundle",
    "bundle": "webpack -p && npm run pack:chrome && npm run pack:base",
    "pack:chrome": "node scripts/pack-chrome.js",
    "pack:base": "node scripts/pack-base.js"
  },
  "repository": {
    "type": "git",
    "url": "https://stash.aexp.com/stash/users/jcros8/repos/parrot/browse"
  },
  "keywords": [
    "parrot",
    "devtools"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "material-ui": "^0.17.0",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "styled-components": "^1.4.6",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "archiver": "^1.3.0",
    "babel-cli": "^6.7.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.15.0",
    "fs-extra": "^2.0.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^19.0.1",
    "node-sass": "^4.5.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.2.8",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.0.0"
  }
}
