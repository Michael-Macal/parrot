{
  "name": "parrot-devtools",
  "version": "1.0.0",
  "contributors": [
    "Zach Ripka",
    "Jack Cross"
  ],
  "description": "Devtool components for easy scenario switching when developing with Parrot.",
  "scripts": {
    "start": "chokidar 'src/**' -c 'npm run build'",
    "clean": "rm -rf build && rimraf dist",
    "lint": "eslint --ignore-path .eslintignore --ext .js --ext .jsx ./",
    "prebuild": "npm run clean && npm run lint",
    "build": "webpack -p && npm run pack:chrome && npm run pack:base",
    "pack:chrome": "node scripts/pack-chrome.js",
    "pack:base": "node scripts/pack-base.js"
  },
  "repository": {
    "type": "git",
    "url": "https://stash.aexp.com/stash/users/jcros8/repos/parrot/browse"
  },
  "keywords": [
    "parrot",
    "devtools"
  ],
  "author": "Zach Ripka",
  "license": "Apache-2.0",
  "devDependencies": {
    "archiver": "^1.3.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.9.0",
    "fs-extra": "^2.0.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^18.1.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.1",
    "react-router-dom": "^4.0.0-beta.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "material-ui": "^0.17.0",
    "parrot-devtools-base": "../parrot-devtools-base",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "url": "^0.11.0"
  }
}
